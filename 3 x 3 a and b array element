#include <stdio.h>

int main() {
    int a[3][3], b[3][3];
    int i, j;

  
    printf("Enter elements for Matrix A (3x3):\n");
    for (i = 0; i < 3; i++) {
        for (j = 0; j < 3; j++) {
          
            scanf("%d", &a[i][j]);
        }
    }
    printf("\nEnter elements for Matrix B (3x3):\n");
    for (i = 0; i < 3; i++) {
        for (j = 0; j < 3; j++) {
           
            scanf("%d", &b[i][j]);
        }
    }
    printf("\nMatrix A:\n");
    for (i = 0; i < 3; i++) {
        for (j = 0; j < 3; j++) {
            printf("%d\t", a[i][j]);
        }
        printf("\n"); 
    }
    printf("\nMatrix B:\n");
    for (i = 0; i < 3; i++) {
        for (j = 0; j < 3; j++) {
            printf("%d\t", b[i][j]);
        }
        printf("\n");  
    }

    return 0;
}
